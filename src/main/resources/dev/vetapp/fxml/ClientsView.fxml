<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" minHeight="-Infinity" minWidth="-Infinity" spacing="10.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dev.vetapp.controllers.ClientsController">
   <children>
      <HBox alignment="CENTER_RIGHT" prefHeight="25.0" prefWidth="200.0">
         <children>
            <Button fx:id="addClientButton" alignment="CENTER" mnemonicParsing="false" onAction="#createNewClientModal" prefHeight="26.0" prefWidth="100.0" style="-fx-background-color: green; -fx-font-weight: bold;" text="%addClient" textAlignment="CENTER" />
         </children>
      </HBox>
      <TableView fx:id="clientsTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <columns>
            <TableColumn fx:id="idColumn" prefWidth="42.399993896484375" text="%ID" />
            <TableColumn fx:id="clientNameColumn" prefWidth="87.20001220703125" text="%clientName" />
            <TableColumn fx:id="clientSurnameColumn" prefWidth="97.5999755859375" text="%clientSurname" />
            <TableColumn fx:id="emailColumn" prefWidth="62.40003967285156" text="%email" />
            <TableColumn fx:id="phoneNumberColumn" prefWidth="98.39996337890625" text="%phoneNumber" />
            <TableColumn fx:id="addressColumn" prefWidth="94.4000244140625" text="%address" />
            <TableColumn fx:id="locationColumn" prefWidth="97.59994506835938" text="%location" />
            <TableColumn fx:id="actionsColumn" prefWidth="60.00006103515625" text="%actions" />
         </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
